@Entity
@Table(name="event")
public class Event extends BaseObject implements Serializable {

    private Long id;
    private Long elementId1;
    private Long elementId2;
    private String maxAmount;
    private Long underElementId;
    private Long overElementId;
    private Long alternateEvent;

    @Id
    @GeneratedValue(strategy= GenerationType.AUTO)
    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    @Column(nullable=false)
    public Long getElementId1() {
        return elementId1;
    }

    public void setElementId1(Long elementId1) {
        this.elementId1 = elementId1;
    }

    @Column(nullable=false)
    public Long getElementId2() {
        return elementId2;
    }

    public void setElementId2(Long elementId2) {
        this.elementId2 = elementId2;
    }

    @Column(nullable=false,length=20)
    public String getMaxAmount() {
        return maxAmount;
    }

    public void setMaxAmount(String maxAmount) {
        this.maxAmount = maxAmount;
    }

    @Column(nullable=false)
    public Long getUnderElementId() {
        return underElementId;
    }

    public void setUnderElementId(Long underElementId) {
        this.underElementId = underElementId;
    }

    @Column(nullable=false)
    public Long getOverElementId() {
        return overElementId;
    }

    public void setOverElementId(Long overElementId) {
        this.overElementId = overElementId;
    }

    @Column(nullable=false)
    public Long getAlternateEvent() {
        return alternateEvent;
    }

    public void setAlternateEvent(Long alternateEvent) {
        this.alternateEvent = alternateEvent;
    }

    public String toString() {
        return null;  //To change body of implemented methods use File | Settings | File Templates.
    }

    public boolean equals(Object o) {
        return false;  //To change body of implemented methods use File | Settings | File Templates.
    }

    public int hashCode() {
        return 0;  //To change body of implemented methods use File | Settings | File Templates.
    }
}
