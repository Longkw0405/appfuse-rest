@Entity
@Table(name="item")
public class Item extends BaseObject implements Serializable {

    private Long id;
    private String itemCode;
    private String name;
    private String amount;
    private Long compoundItemId;
    private Long compoundItemId2;
    private Long elementId;

    @Id
    @GeneratedValue(strategy= GenerationType.AUTO)
    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    @Column(nullable=false,length=20,unique=true)
    public String getItemCode() {
        return itemCode;
    }

    public void setItemCode(String itemCode) {
        this.itemCode = itemCode;
    }

    @Column(nullable=false,length=50)
    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    @Column(nullable=false,length=20)
    public String getAmount() {
        return amount;
    }

    public void setAmount(String amount) {
        this.amount = amount;
    }

    @Column(nullable=false)
    public Long getCompoundItemId() {
        return compoundItemId;
    }

    public void setCompoundItemId(Long compoundItemId) {
        this.compoundItemId = compoundItemId;
    }

    @Column(nullable=false)
    public Long getCompoundItemId2() {
        return compoundItemId2;
    }

    public void setCompoundItemId2(Long compoundItemId2) {
        this.compoundItemId2 = compoundItemId2;
    }

    @Column(nullable=false)
    public Long getElementId() {
        return elementId;
    }

    public void setElementId(Long elementId) {
        this.elementId = elementId;
    }

    public String toString() {
        return null;  //To change body of implemented methods use File | Settings | File Templates.
    }

    public boolean equals(Object o) {
        return false;  //To change body of implemented methods use File | Settings | File Templates.
    }

    public int hashCode() {
        return 0;  //To change body of implemented methods use File | Settings | File Templates.
    }
}
