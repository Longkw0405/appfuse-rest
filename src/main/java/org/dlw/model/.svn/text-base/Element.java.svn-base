@Entity
@Table(name="element")
public class Element extends BaseObject implements Serializable {

    private Long id;
    private String account;
    private String name;
    private Long accountType;
    private Long amountType;
    private boolean list;
    private Long listElementId;
    private Long compoundElementId;
    private Long compoundElementId2;
    private boolean control;

    @Id
    @GeneratedValue(strategy= GenerationType.AUTO)
    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    @Column(nullable=false,length=10)
    public String getAccount() {
        return account;
    }

    public void setAccount(String account) {
        this.account = account;
    }

    @Column(nullable=false,length=50,unique=true)
    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    @Column(nullable=false)
    public Long getAccountType() {
        return accountType;
    }

    public void setAccountType(Long accountType) {
        this.accountType = accountType;
    }

    @Column(nullable=false)
    public Long getAmountType() {
        return amountType;
    }

    public void setAmountType(Long amountType) {
        this.amountType = amountType;
    }

    @Column(nullable=false)
    public boolean isList() {
        return list;
    }

    public void setList(boolean list) {
        this.list = list;
    }

    @Column(nullable=false)
    public Long getListElementId() {
        return listElementId;
    }

    public void setListElementId(Long listElementId) {
        this.listElementId = listElementId;
    }

    @Column(nullable=false)
    public Long getCompoundElementId() {
        return compoundElementId;
    }

    public void setCompoundElementId(Long compoundElementId) {
        this.compoundElementId = compoundElementId;
    }

    @Column(nullable=false)
    public Long getCompoundElementId2() {
        return compoundElementId2;
    }

    public void setCompoundElementId2(Long compoundElementId2) {
        this.compoundElementId2 = compoundElementId2;
    }

    @Column(nullable=false)
    public boolean isControl() {
        return control;
    }

    public void setControl(boolean control) {
        this.control = control;
    }


    public String toString() {
        return null;  //To change body of implemented methods use File | Settings | File Templates.
    }

    public boolean equals(Object o) {
        return false;  //To change body of implemented methods use File | Settings | File Templates.
    }

    public int hashCode() {
        return 0;  //To change body of implemented methods use File | Settings | File Templates.
    }
}
